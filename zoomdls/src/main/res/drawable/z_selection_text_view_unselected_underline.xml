<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-5dp"
        android:right="-5dp"
        android:top="-5dp">
        <shape
            android:shape="rectangle">

            <solid android:color="#ffffff" />

            <stroke
                android:width="1dp"
                android:dashGap="2dp"
                android:dashWidth="2dp"
                android:color="@android:color/black" />
        </shape>
    </item>
</layer-list>